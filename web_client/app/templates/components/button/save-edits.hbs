<BsButton @type="dark" class="m-1 btn-sm text-nowrap"
          @onClick={{this.show_dialog}}
          disabled={{not @changesMade}} >
  Save Edits
</BsButton>


{{#if this.dialogVisible}}
  <Dialog::LogChanges
    @id="logChangesDialog2"
    @oil={{@oil}}
    @submit={{this.submit}}
    @close={{this.close_dialog}}
  />
{{/if}}

