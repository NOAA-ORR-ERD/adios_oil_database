<center>Naphthalenes</center>

<table class="table table-hover table-bordered">
  <thead></thead>
  <tbody id="naphthalenes-tbody">
    {{#if anyDataPresent}}
      <tr>
        <td>C0-N</td>
        <td>{{value-unit oil.alkylated_pahs.c0_n 2}}</td>
      </tr>
      <tr>
        <td>C1-N</td>
        <td>{{value-unit oil.alkylated_pahs.c1_n 2}}</td>
      </tr>
      <tr>
        <td>C2-N</td>
        <td>{{value-unit oil.alkylated_pahs.c2_n 2}}</td>
      </tr>
      <tr>
        <td>C3-N</td>
        <td>{{value-unit oil.alkylated_pahs.c3_n 2}}</td>
      </tr>
      <tr>
        <td>C4-N</td>
        <td>{{value-unit oil.alkylated_pahs.c4_n 2}}</td>
      </tr>
    {{else}}
      <tr>
        <td>No Data</td>
      </tr>
    {{/if}}
  </tbody>
</table>
