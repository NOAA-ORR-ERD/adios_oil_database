<center>Chrysenes</center>

<table class="table table-hover table-bordered">
  <thead></thead>
  <tbody id="chrysenes-tbody">
    {{#if anyDataPresent}}
      <tr>
        <td>C0-C</td>
        <td>{{value-unit oil.alkylated_pahs.c0_c 2}}</td>
      </tr>
      <tr>
        <td>C1-C</td>
        <td>{{value-unit oil.alkylated_pahs.c1_c 2}}</td>
      </tr>
      <tr>
        <td>C2-C</td>
        <td>{{value-unit oil.alkylated_pahs.c2_c 2}}</td>
