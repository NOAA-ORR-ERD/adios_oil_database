<div class="detail-section">
  <div class="table-responsive">
    <strong>GC-TPH (F1 + F2):</strong>

    <table class="table table-hover table-bordered">
      <thead></thead>
      <tbody id="ccmetph-tbody">
        {{#if oil.ccme_tph}}
          <tr>
            <td>n-C8 to n-c10</td>
            <td>{{oil.ccme_tph.n_c8_to_n_c10}}</td>
          </tr>
          <tr>
            <td>n-C10 to n-c12</td>
            <td>{{oil.ccme_tph.n_c10_to_n_c12}}</td>
          </tr>
          <tr>
            <td>n-C12 to n-c16</td>
            <td>{{oil.ccme_tph.n_c12_to_n_c16}}</td>
          </tr>
          <tr>
            <td>n-C16 to n-c20</td>
            <td>{{oil.ccme_tph.n_c16_to_n_c20}}</td>
          </tr>
          <tr>
            <td>n-C20 to n-c24</td>
            <td>{{oil.ccme_tph.n_c20_to_n_c24}}</td>
          </tr>
          <tr>
            <td>n-C24 to n-c28</td>
            <td>{{oil.ccme_tph.n_c24_to_n_c28}}</td>
          </tr>
          <tr>
            <td>n-C28 to n-c34</td>
            <td>{{oil.ccme_tph.n_c28_to_n_c34}}</td>
          </tr>
          <tr>
            <td>n-C34+</td>
            <td>{{oil.ccme_tph.n_c34}}</td>
          </tr>
          <tr>
            <th>Total</th>
            <td>{{oil.ccme_tph.total_tph_gc_detected_tph_undetected_tph}}</td>
          </tr>
        {{else}}
          <tr><td>No Data</td></tr>
        {{/if}}
      </tbody>
    </table>

  </div>
</div>
