<BsTab as |tab|>
  {{#each this.navTabProperties as |props idx|}}
    <tab.pane @title={{props.short_name}}>
      <TabPane::SubSample
        @oil={{@oil}}
        @store={{@oil.store}}
        @editable={{@editable}}
        @submit={{@submit}}
        @sampleTab={{@sampleTab}}
        @categoryTab={{@categoryTab}}
        @updateCategoryTab={{@updateCategoryTab}}
        @sampleName={{props.short_name}}
        @distillationTypes={{@distillationTypes}}
      />
    </tab.pane>
  {{/each}}
</BsTab>
